{% extends 'layout.html.twig' %}

{% block content %}
	
	<form method="post" action="/rest/{{ rested }}/create" >
		{% for colum in columnas %}
			<div>
				<label for="col_{{ colum }}">{{ colum }}</label> 
				 <input id="col_{{ colum }}" name="{{ colum }}" placeholder="{{ colum }}"> 
			</div>
		{% else %}
			<p>No hay registros</p>
		{% endfor %}

		<input type="submit" value="crear">
	</form>
{% endblock %}