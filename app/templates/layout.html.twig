<!DOCTYPE html>
<html>
	<head>
		<title>{{ title }}</title>
		
	
		
		{% block style %}
			<link rel="stylesheet" href="/css/font-awesome.min.css">
			<link rel="stylesheet" href="/css/personal.css">
		{% endblock %}
		
		
	</head>
	<body>
		<p>El t√≠tulo es {{ title }}</p>

		<table>
			<thead>
				<tr>
					{% for colum in columnas %}
						<th>{{ colum }}</th>
					{% endfor %}
				</tr>
			</thead>

			<tbody>
				{% for regi in registros %}
				{#
					<tr>
						
						<td>{{ regi.id_producto }}</td>
						<td>{{ regi.codigo_barras }}</td>
						<td>{{ regi.cantidad }}</td>
						<td>{{ regi.opciones | raw }}</td>
					</tr> #}
					<tr>
					
					{% for colum in columnas %}
						<td>{{ regi[colum] | raw }}</td>
					{% endfor %}
				</tr>
					
				{% else %}
					<p>No hay registros</p>
				{% endfor %}
				
				
			</tbody>
		</table>
	</body>
</html>